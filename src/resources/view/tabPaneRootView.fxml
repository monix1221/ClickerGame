<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx/8.0.111"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:id="tabPane"
         fx:controller="clicker.controllers.TabMainController">
    <tabs>
        <Tab text="Banana"
             fx:id="tabBanana">
            <fx:include
                    source="bananaView.fxml"/>
        </Tab>
        <Tab text="Cherry"
             fx:id="tabCherry">
            <fx:include
                    source="cherryView.fxml"/>
        </Tab>
        <Tab text="Plum"
             fx:id="tabPlum">
            <fx:include
                    source="plumView.fxml"/>
        </Tab>
    </tabs>
</TabPane>
